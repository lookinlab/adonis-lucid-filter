{
  "name": "adonis-lucid-filter",
  "version": "4.1.0",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "main": "./build/providers/LucidFilterProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/commands",
    "build/templates",
    "build/src"
  ],
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "dependencies": {
    "type-fest": "^3.8.0"
  },
  "devDependencies": {
    "@adonisjs/ace": "^11.3.1",
    "@adonisjs/application": "^5.3.0",
    "@adonisjs/config": "^3.0.9",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/events": "^7.2.1",
    "@adonisjs/fold": "^8.2.0",
    "@adonisjs/logger": "^4.1.5",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/profiler": "^6.0.9",
    "@poppinss/dev-utils": "^2.0.3",
    "@types/node": "^18.16.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-plugin-adonis": "^2.1.1",
    "japa": "^3.1.1",
    "knex": "^2.4.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "directories": {
    "test": "test"
  },
  "adonisjs": {
    "types": "adonis-lucid-filter",
    "providers": [
      "adonis-lucid-filter"
    ],
    "commands": [
      "adonis-lucid-filter/build/commands"
    ]
  }
}
