{
  "name": "adonis-lucid-filter",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "version": "5.0.0",
  "engines": {
    "node": ">=20.6.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build/commands",
    "build/src",
    "build/stubs",
    "build/configure.d.ts",
    "build/configure.js",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./types/*": "./build/src/types/*.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "del-cli build",
    "precompile": "npm run lint && npm run clean",
    "postcompile": "npm run copy:stubs && npm run index:commands",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "copy:stubs": "copyfiles \"stubs/**/**/*.stub\" --up=\"1\" build",
    "index:commands": "adonis-kit index build/commands",
    "quick:test": "NODE_DEBUG=\"adonis-lucid-filter\" node --enable-source-maps --loader=ts-node/esm bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "typecheck": "tsc --noEmit",
    "version": "npm run build",
    "release": "np"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.1.1",
    "@adonisjs/core": "^6.2.3",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/lucid": "^20.1.0",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/assert": "^2.1.0",
    "@japa/expect-type": "^2.0.1",
    "@japa/file-system": "^2.2.0",
    "@japa/runner": "^3.1.1",
    "@japa/snapshot": "^2.0.4",
    "@swc/core": "^1.4.1",
    "@types/lodash": "^4.14.202",
    "c8": "^9.1.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "np": "^9.2.0",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "type-fest": "^4.10.2"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.3",
    "@adonisjs/lucid": "^20.1.0",
    "lodash": "^4.17.21"
  },
  "author": "LookinGit",
  "license": "MIT",
  "homepage": "https://github.com/lookinlab/adonis-lucid-filter#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "keywords": [
    "adonisjs",
    "adonisjs-filter",
    "adonisjs-lucid",
    "adonisjs-lucid-filter",
    "adonis",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "directories": {
    "test": "test"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "bin/**"
    ]
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types/*.ts",
      "./src/bindings/model_query_builder.ts",
      "./commands/make_filter.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
