{
  "name": "adonis-lucid-filter",
  "version": "4.1.0",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "main": "./build/providers/LucidFilterProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/commands",
    "build/templates",
    "build/src"
  ],
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "dependencies": {
    "type-fest": "^2.3.4"
  },
  "devDependencies": {
    "@adonisjs/ace": "^11.0.1",
    "@adonisjs/application": "^5.1.2",
    "@adonisjs/config": "^3.0.2",
    "@adonisjs/core": "^5.1.6",
    "@adonisjs/events": "^7.0.1",
    "@adonisjs/fold": "^8.1.2",
    "@adonisjs/logger": "^4.0.2",
    "@adonisjs/lucid": "^16.0.2",
    "@adonisjs/mrm-preset": "^4.1.0",
    "@adonisjs/profiler": "^6.0.3",
    "@poppinss/dev-utils": "^1.1.2",
    "@types/node": "^16.9.4",
    "copyfiles": "^2.4.1",
    "eslint": "^7.25.0",
    "eslint-plugin-adonis": "^1.3.1",
    "japa": "^3.1.1",
    "knex": "^0.95.4",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "directories": {
    "test": "test"
  },
  "adonisjs": {
    "types": "adonis-lucid-filter",
    "providers": [
      "adonis-lucid-filter"
    ],
    "commands": [
      "adonis-lucid-filter/build/commands"
    ]
  }
}
